<div class="col-md-8 col-md-offset-2">
  <div class="row">
    <div class="panel panel-default">
      <ul class="pricing-table">
        <li class="title" style="font-size:2em"><%= @listing.title %></li>
        <li class="price">$<%= @listing.rate_cents %>/week</li>
        <li class="bullet-item">
          <%= @listing.address %><br>
          <%= @listing.city %>, <%= @listing.state %>
          <%= @listing.zip_code %>
        </li>
        <li class="description"><%= @listing.description %></li>
        <li class="cta-button"><a class="button radius"href="#">Available Soon</a></li>
      </ul>
    </div>
  </div>
</div>
